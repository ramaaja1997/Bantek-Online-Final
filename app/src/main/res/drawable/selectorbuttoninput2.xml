<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#FFF"></solid>
            <corners android:radius="25dp"></corners>
            <stroke
                android:width="1dp"
                android:color="#13619A">
            </stroke>
            <padding
                android:top="0dp"
                android:right="10dp"
                android:left="10dp"
                android:bottom="0dp">
            </padding>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#FFF"></solid>
            <corners android:radius="25dp"></corners>
            <stroke
                android:width="1dp"
                android:color="#13619A">
            </stroke>
            <padding
                android:top="0dp"
                android:right="10dp"
                android:left="10dp"
                android:bottom="0dp">
            </padding>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="#FFF"></solid>
            <corners android:radius="25dp"></corners>
            <stroke
                android:width="1dp"
                android:color="#13619A">
            </stroke>
            <padding
                android:top="0dp"
                android:right="10dp"
                android:left="10dp"
                android:bottom="0dp">
            </padding>
        </shape>
    </item>

</selector>